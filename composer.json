{
  "name": "bean-project/core-bundle",
  "type": "symfony-bundle",
  "description": "Symfony BeanCoreBundle",
  "keywords": ["bundle", "bean"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Pierre",
      "email": "peter@bean-project.org"
    }
  ],
  "require": {
    "symfony/framework-bundle": "^3.1",
    "sonata-project/core-bundle": "^3.0"
  },
  "require-dev": {
    "doctrine/mongodb-odm": "~1.0",
    "jackalope/jackalope-doctrine-dbal": "~1.1",
    "symfony/phpunit-bridge": "~2.7|~3.0"
  },
  "suggest": {
    "sonata-project/doctrine-orm-admin-bundle": "~2.3",
    "liip/imagine-bundle": "~0.9"
  },
  "conflict": {
    "jms/serializer": "<0.13"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": { "Bean\\Bundle\\CoreBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
